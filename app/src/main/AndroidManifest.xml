<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.travelerbuddy.feri.travelersbuddy">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/MyMaterialTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DogodkiActivity"
            android:label="@string/title_activity_dogodki"
            android:theme="@style/MyMaterialTheme" />
        <activity
            android:name=".kovcek_items"
            android:label="@string/title_activity_kovcek_items"
            android:theme="@style/MyMaterialTheme"></activity>
    </application>

</manifest>
